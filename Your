successMessageCorner.Parent = successMessageFrame

-- Fading Function
local function fadeIn(gui, duration)
    gui.Visible = true
    local startTime = tick()
    local function update()
        local elapsedTime = tick() - startTime
        local transparency = math.clamp(elapsedTime / duration, 0, 1)
        gui.BackgroundTransparency = 1 - transparency
        for _, child in ipairs(gui:GetChildren()) do
            if child:IsA("TextLabel") then
                child.TextTransparency = 1 - transparency
            end
        end
    end
    while tick() - startTime < duration do
        update()
        game:GetService("RunService").Heartbeat:Wait()
    end
    update()
end

-- Submit Button Functionality
SubmitButton.MouseButton1Click:Connect(function()
    if KeySystem.Text == "FREE_njkgb78ghokl" then
        -- Show success message with fade-in
        fadeIn(successMessageFrame, 2)

        -- Hide the success message after 3 seconds
        wait(3)
        successMessageFrame.Visible = false

        -- Destroy the GUI before loading the script
        wait(0.1)
        screenGui:Destroy()

        -- Load the external script after destroying the GUI
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/sdhnjkf/ww/refs/heads/main/w'), true))()
    else
        -- Show error message with fade-in
        fadeIn(errorMessageFrame, 2)

        -- Wait for 3 seconds before fading out the error message
        wait(3)
        errorMessageFrame.Visible = false
    end
end)

-- Get Key Button Functionality
GetKeyButton.MouseButton1Click:Connect(function()
    setclipboard("https://loot-link.com/s?UPUqTpjo")
end)

-- Fade-in for the main GUI
fadeIn(frame, 2)
